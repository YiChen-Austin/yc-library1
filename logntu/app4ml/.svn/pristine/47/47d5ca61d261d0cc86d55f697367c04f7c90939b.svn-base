input.ui-button {margin-bottom: .4em;margin-top:.4em}
body {font-family: "宋体";font-size: 12px;margin-left: 10px;margin-top: 0px;margin-right: 0px;margin-bottom: 0px;}
.mainTable input[type=text],input[type=password]{height:16px;width: 110px;}
.fieldset{width: 300px;height: 294px;}
/*.rightFieldSet{height:260px;}*/
.errorMessage {font-family: verdana, arial, helvetica, sans-serif;font-size: 12px;color: #FF3300;vertical-align: bottom;}
.redtd{
  background-color: red;
}
.bluetr{
  background-color: yellow;
}
.errbox{ font-size:12px; font-family:"宋体"; background:#f1fcfd;  line-height:18px; 	color: #2f5cc1;border:1px solid #addef4;padding:10px;width:97%;margin-top: 10px;margin-bottom:10px;}
.errbox ul{list-style:none;top:10px;left:10px;position:relative;top:-10px;left:-30px; margin-bottom: -10px;}
.errbox ul li{LINE-HEIGHT: 20px; letter-spacing: 2px;}

.rectangle{padding:10px;width:96.5%;margin-top: 10px;}
.tags {
	PADDING-RIGHT: 0px;
	 PADDING-LEFT: 0px;
	  PADDING-BOTTOM: 0px;
	   MARGIN: 0px 0px 0px 10px;
	    WIDTH: 80%;
	     PADDING-TOP: 0px;
	      HEIGHT: 23px
}
.tags LI {
	FLOAT: left;
	MARGIN-RIGHT: 1px;
	LIST-STYLE-TYPE: none;
	HEIGHT: 23px;
	background-image: url(../images/main/tagleft.gif);
	background-repeat: no-repeat;
	background-position: left bottom;
}
.tags LI A {
	PADDING-RIGHT: 10px;
	PADDING-LEFT: 10px;
	FLOAT: left;
	PADDING-BOTTOM: 0px;
	COLOR: #333333;
	LINE-HEIGHT: 23px;
	PADDING-TOP: 0px;
	HEIGHT: 23px;
	TEXT-DECORATION: none;
	background-image: url(../images/main/tagright.gif);
	background-repeat: no-repeat;
	background-position: right bottom;
}

.tags LI.emptyTag {
	BACKGROUND: none transparent scroll repeat 0% 0%; WIDTH: 4px
}
.tags LI.selectTag {
	BACKGROUND-POSITION: left top; MARGIN-BOTTOM: -2px; POSITION: relative; HEIGHT: 25px
}
.tags LI.selectTag A {
	BACKGROUND-POSITION: right top;
	COLOR: #333333;
	LINE-HEIGHT: 25px;
	HEIGHT: 25px;
	font-weight: bold;
}
.tagContent {
	/* DISPLAY: none; */
	COLOR: #474747;
	padding: 10px;
	border: 1px solid #aecbd4;
	background-color: #fafafa;
	width: 97%;
	margin-bottom: 10px;
}
.hide {
	display: none;
}
