body {background-color:#fff;}
html, body {height:100%;}
html body{font:12px Arial, Helvetica, sans-serif;color:#333333}
html>body{font:12px Arial, Helvetica, sans-serif;color:#333333}
img{border:none;}
#TB_overlay {
	position: absolute;
	top: 0;
	left: 0;
	z-index:100;
	width: 100%;
	height: 100%;
	background-color:#CCC;
	filter:alpha(opacity=60);
	-moz-opacity: 0.6;
	opacity: 0.6;
}
#TB_window {
	top: 0px;
	left: 0px;
	position: fixed;
	_position: absolute;
	
	z-index: 102;
	color:#000000;
	display:none;
	
}
#TB_caption{
	height:25px;
	padding:10px 30px 10px 25px;
}
#TB_closeWindow{
	

width:53px;height:53px; display:block;
text-decoration:none;
	
}
#TB_closeWindow:hover{
	
text-decoration:none;
background:url(../images/iphone/exiton.jpg) no-repeat;	
}

#TB_closeAjaxWindow{
	
	margin-bottom:1px;
	text-align:center;

	position:absolute;
	top:450px;
	width:100%;
	z-index:120;

	
}
#TB_ajaxContent{
	padding:2px 15px 15px 15px;
	overflow:auto;
}

#TB_load{
	text-align: center;
	position: absolute;
	top: 50%;
	left: 0px;
	width: 100%;
	overflow: visible;
	visibility: visible;
	display: block;
	z-index:101;
}
#TB_loadContent{
	margin-left: -175px;
	position: absolute;
	top: -274px;
	left: 50%;
	width: 350px;
	height: 547px;
	visibility: visible;
}