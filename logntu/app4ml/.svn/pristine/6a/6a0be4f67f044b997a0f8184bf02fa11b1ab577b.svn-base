	public boolean validData() {
		// 地址
		if (addressId == null || addressId <= 0)
			return false;
		// 产品id，必有
		if (goodsIds == null || goodsIds.length() <= 0)
			return false;
		// 产品数量，必有
		if (quantities == null || quantities.length() <= 0)
			return false;
		// 运费,必有
		// if (logisticsIds == null || logisticsIds.length() <= 0)
		// return false;
		if (quantities.split(",").length != goodsIds.split(",").length)
			return false;
		if (specIds != null && specIds.split(",").length != goodsIds.split(",").length)
			return false;
		if (promoIds != null && promoIds.split(",").length != goodsIds.split(",").length)
			return false;
		return true;
	}

