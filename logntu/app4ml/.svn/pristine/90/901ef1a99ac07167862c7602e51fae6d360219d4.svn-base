html {
  padding:0px;
  margin:0px;
}
body { 
  font-size: 12px;
  font-family: Verdana, Arial, SunSans-Regular, Sans-Serif;
  color:#000000;
  padding:0px 0px;
  margin:0px;
}
.body_login {
  font-size: 12px;
  font-family: Verdana, Arial, SunSans-Regular, Sans-Serif;
  color:#564b47;
  padding:0px 0px;
  margin:0px;
}
HR {
	COLOR: #339900; HEIGHT: 1px
}
TH,TD {
	FONT-SIZE: 12px;
	COLOR: #2F5CC1;
}
SELECT {
	BACKGROUND-COLOR: #FFFFFF;
	WIDTH: 120px;
	COLOR: #00485B;
	FONT-SIZE: 12px;
}
INPUT.TextInput {
	BACKGROUND-COLOR: #D6E6FF; 
	BORDER-RIGHT: black 0px solid; 
	BORDER-TOP: black 0px solid; 
	FONT-SIZE: 12px; 
	IME-MODE: active; 
	BORDER-LEFT: black 0px solid; 
	COLOR: #00485B;
	BORDER-BOTTOM: #2F5CC1 1px solid ; 
	FONT-FAMILY: "ו"; 
	HEIGHT: 18px
}
INPUT.Lonin {
	BACKGROUND-COLOR:#E6EDF9; 
	BORDER-RIGHT: black 0px solid; 
	BORDER-TOP: black 0px solid; 
	FONT-SIZE: 12px; 
	IME-MODE: active; 
	BORDER-LEFT: black 0px solid; 
	COLOR: #000000; 
	BORDER-BOTTOM: #2F5CC1 1px solid ; 
	HEIGHT: 18px
}
INPUT.BtnAction {
	BORDER-RIGHT: #31557f 1px solid; 
	PADDING-RIGHT: 1px; 
	BORDER-TOP: #cce3ff 1px solid; 
	PADDING-LEFT: 1px; 
	FONT-WEIGHT: normal; 
	FONT-SIZE: 12px; 
	BACKGROUND: #9eaccd; 
	PADDING-BOTTOM: 2px; 
	MARGIN: 2px; 
	WORD-SPACING: 0px; 
	VERTICAL-ALIGN: middle; 
	BORDER-LEFT: #cce3ff 1px solid; 
	CURSOR: hand; 
	COLOR: #000000; 
	PADDING-TOP: 2px; 
	BORDER-BOTTOM: #31557f 1px solid; 
	LETTER-SPACING: 0px; 
	HEIGHT: 20px
}
A:link {
	COLOR: #0000a0; TEXT-DECORATION: underline
}
A:active {
	COLOR: #0000a0; TEXT-DECORATION: underline
}
A:visited {
	COLOR: #0000a0; TEXT-DECORATION: underline
}
A:hover {
	COLOR: red; TEXT-DECORATION: underline
}

.td_title {
	BORDER-RIGHT: #99CAD8 1px solid;
	BORDER-TOP: #99CAD8 1px solid;
	BORDER-LEFT: #99CAD8 1px solid;
	BORDER-BOTTOM: #99CAD8 1px solid;
	background:url("../images/tab_bg.gif" );
	COLOR: #0000a0;
	FONT-WEIGHT: normal;
	TEXT-ALIGN: center;
	HEIGHT:20px;
}

.td_border {
	BORDER-RIGHT: #99CAD8 1px solid;
	BORDER-TOP: #99CAD8 1px solid;
	BORDER-LEFT: #99CAD8 1px solid;
	BORDER-BOTTOM: #99CAD8 1px solid;
	HEIGHT:18px;
}
.errbox{ border:1px dashed #BE0000;width:800px;background:#FFDECE;}
.inputbox,
.textareaBox{ border:1px solid #77B2DD;width:115px;height:17px;line-height:17px;}
.textareaBox{ width:280px;height:40px;}
.inputboxlen{border:1px solid #77B2DD;height:17px;line-height:17px;width:300px;}
.inputboxshr{border:1px solid #77B2DD;height:17px;line-height:17px;width:50px;}