$(function(){
	$(":button,:reset,:submit").addClass("ui-button ui-state-default ui-corner-all").hover(function(){
        $(this).addClass("ui-state-hover");
    }, function(){
        $(this).removeClass("ui-state-hover");
    }).mousedown(function(){
        $(this).addClass("ui-state-active");
    }).mouseup(function(){
        $(this).removeClass("ui-state-active");
    });
  //$("input[type!=text]").addClass("text ui-widget-content ui-corner-all");
   // $("select").addClass("select ui-widget-content ui-corner-all");
});
