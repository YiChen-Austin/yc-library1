/*css*/
.lt-header{
    height: 90px;
    background: #fff;
    position: fixed;
    width: 100%;
    top: 0px;
    left: 0px;
    z-index: 20;
}
.lt-header:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}
.logo{
    display: inline-block;
    margin-top: 15px;
}
.lt-header .nav{
    list-style: none;
    margin: 0px;
    padding: 0px;
    margin-left: 100px;
    margin-top: 25px;
}
.lt-header .nav li .item{
    display: block;
    float: left;
    padding: 0 30px;
    font-size: 16px;
    text-align: center;
}
.lt-header .nav li .item:hover p{
    color: #cb3f06
}
.lt-header .nav li .item:hover span{
    color: #cb3f06
}
.lt-header .nav li .item span{
    color: #222222;
    font-size:18px;
}
.lt-header .nav li .item p{
    color: #777777;
    font-family:Arial Regular ;
    text-transform:Uppercase;
}
.lt-header .nav li .cur span{
    color: #cb3f06!important;
}
.lt-header .nav li .cur p{
    color: #e69878!important;
    
}
.lt-header .user_info{
    font-size: 14px;
    margin-right: 36px;
    margin-top: 30px;
}
.lt-header .user_info a{
    color: #221e1f;
    padding-left: 10px;
    text-decoration: none;
    outline: 0;
}
.lt-header .user_info span{
    margin-left: 12px;
    color: #221e1f;
}
/*轮播*/
.lt-banner{
    width: 100%;
}
.swiper-container {
    width: 100%;
    height: 100%;
}
.swiper-slide {
    text-align: center;
    font-size: 18px;
    background: #fff;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}

.swiper-slide .title {
    font-style: italic;
    font-size: 42px;
    margin-top: 80px;
    margin-bottom: 0;
    line-height: 45px;
}
.swiper-slide p {
    font-style: italic;
    font-size: 25px;
}
.pagination {
    position: absolute;
    z-index: 20;
    right: 100px;
    top: 50%;
}
.swiper-pagination-switch {
    display: block;
    width: 10px;
    height: 10px;
    border-radius: 8px;
     background: url("../img/carousel-icon.png") no-repeat 0px -11px;
    margin: 0 0px 10px;
    cursor: pointer;
}
.swiper-active-switch {
    background: url("../img/carousel-icon.png") no-repeat 0px 0px;
}
